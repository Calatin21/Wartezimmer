@model Queue<Patient>?
    @{
        Layout = null;
    }
<head> 
    <title>Patienten Liste</title>   
    <nav>
        <a asp-action="Index"><button type="button">Home</button></a>
        <a asp-action="PatientenListe"><button type="button">Warteraum</button></a>
        <a asp-action="Arztsicht"><button type="button">Arzt</button> </a>
        <a asp-action="Eingabe"><button type="button">Arzthelfer</button></a>
    </nav>
</head>
<body>
   <h2>Patienten Liste:</h2>
   <table>
       <thead>
            <tr><th>PNr</th><th>Anrede</th><th>Vorname</th><th>Name</th><th>Postleitzahl</th><th>Ort</th><th>Strasse</th><th>Hausnummer</th><th>Actions</th>
       </thead>
       <tbody>  
           @foreach(Patient q in Model){
         <tr>  
             <td>@q.PNr</td>
             <td>@q.Anrede</td>
             <td>@q.Vorname</td>
             <td>@q.Name</td>
             <td>@q.Plz</td>
             <td>@q.Ort</td>
             <td>@q.Strasse</td>
             <td>@q.Hausnummer</td>
             <td>
                 <form asp-action="Delete" asp-route-id="@q.PNr" method="post">
                        <button type="submit">Delete</button>
                        </form>
              </td>
         </tr>
           }        
       </tbody>
	</table>
</body>
